	file = {Full Text PDF:C\:\\Users\\simon\\Zotero\\storage\\S822WP39\\Rao et al. - 2023 - Medical Diagnosis Reimagined as a Process of Bayesian Reasoning and Elimination.pdf:application/pdf},
}

