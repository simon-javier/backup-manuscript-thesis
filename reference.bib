	file = {Full Text PDF:/home/simon/Zotero/storage/TTTUNV32/Cafaro et al. - 2025 - Combined semiquantitative nail-enthesis complex ultrasonography and capillaroscopy in psoriasis and.pdf:application/pdf},
}

