	abstract = {Diseases of human nails are a kind of deformities in nails. Nail has its own various class of diseases, which have their own set of symptoms, signs, causes and eﬀects that may or may not impute to other medical conditions. In this study, four types of nail diseases have been addressed, represented by healthy nail, nail hyperpigmentation, nail clubbing and nail fungus. The images resulting from these nail diseases are classiﬁed using ﬁve pre-trained models of deep Convolutional Neural Network (CNN), namely (AlexNet, Vgg16, GoogleNet, ResNet50 and DenseNet201). Six metrics are calculated to evaluate the performance of each Transfer Learning (TL) model, which are accuracy, recall, speciﬁcity, precision, F-score and time. The TL models are implemented and trained based on MATLAB programming software. The dataset has been tested and the accuracy of each case has been reported. The accuracies based on AlexNet, Vgg16, and GoogleNet are 92.5\%, 87.5\%, 93.98\%, while that based on ResNet50 and DenseNet201 has accuracy of 96.39\%.},
	language = {en},
	urldate = {2025-07-18},
	publisher = {ICIC International 学会},
	author = {Abdulhadi, Jumana and Al-Dujaili, Ayad and Humaidi, Amjad Jaleel and Fadhel, Mohammed Abdul-Raheem},
	year = {2021},
	file = {PDF:/home/simon/Zotero/storage/2CDGGW6Z/Abdulhadi et al. - 2021 - Human Nail Diseases Classification Based on Transfer Learning.pdf:application/pdf},
}

@article{barney_detecting_2025,
	title = {Detecting {Fentanyl} in {Human} {Nails} and {Exploring} the {Inhibition} {Mechanism} of {Fentanyl} on {Butyrylcholinesterase} via {Spectroscopy} and {Machine} {Learning}},
	url = {https://hdl.handle.net/2346/102268},
	abstract = {Fentanyl, a synthetic, illicitly manufactured opioid, has been the cause of a continuous, steep increase of drug overdose cases across recent decades. Fentanyl is extremely potent, lethal, and dangerous. Traces of it can be found in bodies of water across the world, making it a danger for anyone who may encounter it, not just drug manufactures, dealers, or users. Research into detection methods and biological effects of fentanyl are highly important today. Here, we discuss two different projects related to fentanyl. In one project, we explore a novel detection method using human nails as a sample matrix by using ATR-FTIR spectroscopy together with PLS-DA and SVM-DA machine learning techniques to determine if nails containing fentanyl can be distinguished from those without. In another, ongoing project, we attempt to discover the currently unknown inhibition mechanism of two fentanyl analogues on butyrylcholinesterase through enzymatic assay, Lineweaver-Burk charts, and Raman spectroscopy combined with PCA and PLS machine learning techniques. The results of the first project demonstrate that simple visual examination of spectra is not very helpful when attempting to distinguish between control and fentanyl groups. However, both machine learning models performed extremely well when asked to categorize the data into the two groups. Both models performed with high specificity, sensitivity, and overall accuracy at all stages of model creation, optimization, and validation. This excellent performance only degraded slightly when exposed to unseen data, only further proving their robustness. ROC curves for the PLS-DA model indicated an overall accuracy of 98\% for cross validation and an overall accuracy of 94\% for external validation. For the SVM-DA model, the ROC curves indicated 97\% overall accuracy for cross validation and 89\% for external validation. When classification was considered at the donor level—i.e., determining whether the donor of the nail sample was using fentanyl—all donors were correctly classified. The second project discussed here is still ongoing. Firstly, BChE was inhibited with 2 different fentanyl analogues and enzymatic activity was assessed via the Ellman assay method. These results were used to create Lineweaver-Burk charts to determine if a main inhibition mechanism could be observed. Lineweaver-Burk analysis indicated mixed type inhibition for one fentanyl and no inhibition for the other. The Raman spectra of these samples were then collected and processed using PCA and PLS machine learning methods. These results demonstrated that variation between the sample groups could be observed. As this project continues, we aim to compare the fentanyl-inhibited samples with samples inhibited with compounds of known mechanism. We hope to observe similarities between the fentanyl samples and known mechanism samples that agree with the Lineweaver-Burk results to determine the inhibition mechanism of fentanyl.},
	language = {English},
	urldate = {2025-07-18},
	author = {Barney, Aubrey Fallon},
	month = may,
	year = {2025},
	file = {Full Text PDF:/home/simon/Zotero/storage/TWEDM7W2/Barney - 2025 - Detecting Fentanyl in Human Nails and Exploring the Inhibition Mechanism of Fentanyl on Butyrylcholi.pdf:application/pdf},
}

@article{abu_elhamd_clinical_2022,
	title = {Clinical, dermoscopic, and histopathological evaluations of patients with nail disorders},
	volume = {21},
	copyright = {http://onlinelibrary.wiley.com/termsAndConditions\#vor},
	issn = {1473-2130, 1473-2165},
	url = {https://onlinelibrary.wiley.com/doi/10.1111/jocd.14616},
	doi = {10.1111/jocd.14616},
	abstract = {AbstractObjectiveThis study aimed to assess the possible role of dermoscopy in the diagnosis of different nail disorders compared with the clinical and histopathological diagnosis.MethodsThis study included 104 patients with nail disorders of both sex and any age. All the patients were subjected to a detailed medical history taken, clinical general examination, clinical and dermoscopic examinations of 20 nails. Potassium hydroxide examination was used to examine all patients with suspicious of onychomycosis. Histopathological evaluation was used to examine clinically ambiguous nail disorders.ResultsThe most common nail diseases were onychomycosis (54.81\%) followed by nail psoriasis (19.2\%) and nail lichen planus (3.8\%).ConclusionClinical evaluation was important in the diagnosis of different nail diseases. Nail dermoscopy could confirm clinical diagnosis and help in getting an accurate diagnosis of nail diseases and guide in the management of nail diseases by permitting better visualization of nail structure and pathology. The histopathological examination provided the most definitive method in reaching an accurate diagnosis due to it could provide etiologic, diagnostic, and prognostic information. There was a good agreement between clinical, dermoscopic, and histopathological examinations for accurate diagnosis of different nail diseases.},
	language = {en},
	number = {1},
	urldate = {2025-07-18},
	journal = {Journal of Cosmetic Dermatology},
	author = {Abu El‐Hamd, Mohammed and Yassin, Fatma El‐Zahraa Salah El‐Deen and El‐hamid, Nehal Hussein Abd and El‐Sharkawy, Reham Ezz El Dawla},
	month = jan,
	year = {2022},
	note = {Publisher: Wiley},
	pages = {347--357},
	file = {Full Text PDF:/home/simon/Zotero/storage/ESYTHCAE/Abu El‐Hamd et al. - 2022 - Clinical, dermoscopic, and histopathological evaluations of patients with nail disorders.pdf:application/pdf},
}

@misc{huang_review_2023,
	title = {A review of uncertainty quantification in medical image analysis: probabilistic and non-probabilistic methods},
	shorttitle = {A review of uncertainty quantification in medical image analysis},
	url = {http://arxiv.org/abs/2310.06873},
	doi = {10.48550/arXiv.2310.06873},
	abstract = {The comprehensive integration of machine learning healthcare models within clinical practice remains suboptimal, notwithstanding the proliferation of high-performing solutions reported in the literature. A predominant factor hindering widespread adoption pertains to an insufficiency of evidence affirming the reliability of the aforementioned models. Recently, uncertainty quantification methods have been proposed as a potential solution to quantify the reliability of machine learning models and thus increase the interpretability and acceptability of the result. In this review, we offer a comprehensive overview of prevailing methods proposed to quantify uncertainty inherent in machine learning models developed for various medical image tasks. Contrary to earlier reviews that exclusively focused on probabilistic methods, this review also explores non-probabilistic approaches, thereby furnishing a more holistic survey of research pertaining to uncertainty quantification for machine learning models. Analysis of medical images with the summary and discussion on medical applications and the corresponding uncertainty evaluation protocols are presented, which focus on the specific challenges of uncertainty in medical image analysis. We also highlight some potential future research work at the end. Generally, this review aims to allow researchers from both clinical and technical backgrounds to gain a quick and yet in-depth understanding of the research in uncertainty quantification for medical image analysis machine learning models.},
	urldate = {2025-07-18},
	publisher = {arXiv},
	author = {Huang, Ling and Ruan, Su and Xing, Yucheng and Feng, Mengling},
	month = oct,
	year = {2023},
	note = {arXiv:2310.06873 [eess]},
	keywords = {Computer Science - Computer Vision and Pattern Recognition, Electrical Engineering and Systems Science - Image and Video Processing},
	annote = {Comment: arXiv admin note: substantial text overlap with arXiv:2210.03736 by other authors},
	file = {Preprint PDF:/home/simon/Zotero/storage/PVCCBXSJ/Huang et al. - 2023 - A review of uncertainty quantification in medical image analysis probabilistic and non-probabilisti.pdf:application/pdf;Snapshot:/home/simon/Zotero/storage/KN9ZRGEB/2310.html:text/html},
}

