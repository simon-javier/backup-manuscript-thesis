	file = {Full Text:/home/simon/Zotero/storage/WH5PA2XZ/Lim et al. - 2021 - Diagnosis of Onychomycosis From Conventional Techniques and Dermoscopy to Artificial Intelligence.pdf:application/pdf},
}

