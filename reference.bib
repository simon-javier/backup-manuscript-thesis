	keywords = {Computer Science - Computer Vision and Pattern Recognition},
	file = {Preprint PDF:/home/simon/Zotero/storage/BXJJLZER/Jahn et al. - 2025 - Comparison of marker-less 2D image-based methods for infant pose estimation.pdf:application/pdf;Snapshot:/home/simon/Zotero/storage/HINSG5XW/2410.html:text/html},
}

