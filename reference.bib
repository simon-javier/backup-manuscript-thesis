	note = {Publisher: Springer Science and Business Media LLC},
	file = {Full Text:/home/simon/Zotero/storage/BX9ILRJL/Folle et al. - 2023 - DeepNAPSI multi-reader nail psoriasis prediction using deep learning.pdf:application/pdf},
}

@article{kemenes_advancement_2025,
	title = {Advancement and independent validation of a deep learning-based tool for automated scoring of nail psoriasis severity using the modified nail psoriasis severity index},
