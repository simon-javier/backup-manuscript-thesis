	keywords = {dermatoscopy, dermoscopy, melanoma, melanonychia, nail, pigmentation},
	file = {Full Text PDF:C\:\\Users\\simon\\Zotero\\storage\\G2BTIKB8\\Lallas et al. - 2023 - Seven Plus One Steps to Assess Pigmented Nail Bands (Melanonychia Striata Longitudinalis).pdf:application/pdf},
