	keywords = {Computer Science - Computer Vision and Pattern Recognition},
	file = {Preprint PDF:/home/simon/Zotero/storage/NU4QFLT8/Huang et al. - 2021 - Invariant Representation Learning for Infant Pose Estimation with Small Data.pdf:application/pdf;Snapshot:/home/simon/Zotero/storage/LTQF2VWG/2010.html:text/html},
}

