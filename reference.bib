	journal = {Frontiers in Medicine},
	author = {Kemenes, Stephan and Chang, Liu and Schlereth, Maja and Noversa De Sousa, Rita and Minopoulou, Ioanna and Fenzl, Pauline and Corte, Giulia and Yalcin Mutlu, Melek and Höner, Michael Wolfgang and Sagonas, Ioannis and Coppers, Birte and Liphardt, Anna-Maria and Simon, David and Kleyer, Arnd and Folle, Lukas and Sticherling, Michael and Schett, Georg and Maier, Andreas and Fagni, Filippo},
	month = apr,
	year = {2025},
	note = {Publisher: Frontiers Media SA},
	file = {Full Text PDF:/home/simon/Zotero/storage/VMF383GX/Kemenes et al. - 2025 - Advancement and independent validation of a deep learning-based tool for automated scoring of nail p.pdf:application/pdf},
}

