	journaltitle = {Dermatology Practical \& Conceptual},
	author = {Lallas, Aimilios and Korecka, Katarzyna and Apalla, Zoe and Sgouros, Dimitrios and Liopyris, Konstantinos and Argenziano, Giuseppe and Thomas, Luc},
	urldate = {2025-09-17},
