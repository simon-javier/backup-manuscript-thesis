	volume = {13},
	copyright = {http://creativecommons.org/licenses/by/3.0/},
	issn = {2071-1050},
	shorttitle = {Artificial {Intelligence} in the {Industry} 4.0, and {Its} {Impact} on {Poverty}, {Innovation}, {Infrastructure} {Development}, and the {Sustainable} {Development} {Goals}},
