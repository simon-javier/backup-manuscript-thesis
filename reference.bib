	keywords = {Computer Science - Machine Learning, Computer Science - Neural and Evolutionary Computing, Mathematics - Optimization and Control},
	file = {Preprint PDF:C\:\\Users\\simon\\Zotero\\storage\\WIMBZPKW\\Loshchilov and Hutter - 2019 - Decoupled Weight Decay Regularization.pdf:application/pdf;Snapshot:C\:\\Users\\simon\\Zotero\\storage\\2EEGFR6J\\1711.html:text/html},
