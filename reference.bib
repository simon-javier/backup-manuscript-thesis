	keywords = {Computer Science - Artificial Intelligence, Computer Science - Computer Vision and Pattern Recognition},
	file = {Preprint PDF:/home/simon/Zotero/storage/2PVI739H/Shoaib et al. - 2023 - Revolutionizing Global Food Security Empowering Resilience through Integrated AI Foundation Models.pdf:application/pdf},
}

